  "keywords": [
    "bitcoin",
    "multisig",
    "wallet",
    "client",
    "bch"
  ],
  "engine": "node >= 8.0.0",
  "main": "index.js",
  "repository": {
    "url": "git@github.com:owstack/bch-wallet-client.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/owstack/bch-wallet-client/issues"
  },
  "dependencies": {
    "async": "^2.5.0",
    "bip38": "^2.0.1",
    "bch-lib": "https://github.com/owstack/bch-lib",
    "bch-mnemonic": "https://github.com/owstack/bch-mnemonic",
    "bch-payment-protocol": "https://github.com/owstack/bch-payment-protocol",
    "json-stable-stringify": "^1.0.0",
    "lodash": "^4.17.4",
    "preconditions": "^2.2.0",
    "sjcl": "1.0.7",
    "superagent": "^3.4.1"
  },
  "devDependencies": {
    "bch-wallet-service": "https://github.com/owstack/bch-wallet-service",
    "browserify": "^14.4.0",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "grunt": "~1.0.1",
    "grunt-jsdox": "^0.1.7",
    "istanbul": "*",
    "mocha": "^4.0.0",
    "sinon": "^4.0.0",
    "supertest": "^3.0.0",
    "tingodb": "^0.5.1",
    "uglify": "^0.1.1",
    "uuid": "^3.1.0"
  },
  "scripts": {
    "start": "node app.js",
    "coverage": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- --reporter spec test",
    "test": "./node_modules/.bin/mocha",
    "coveralls": "./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "docs": "./node_modules/.bin/jsdox lib/* lib/common lib/errors -o docs && cat README.header.md  docs/*.md LICENSE > README.md"
